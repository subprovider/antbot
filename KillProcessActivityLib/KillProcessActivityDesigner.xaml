<sap:ActivityDesigner x:Class="ktds.Ant.Activities.KillProcessActivityDesigner"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:sap="clr-namespace:System.Activities.Presentation;assembly=System.Activities.Presentation"
    xmlns:sapv="clr-namespace:System.Activities.Presentation.View;assembly=System.Activities.Presentation">
    <Grid>
        <StackPanel Orientation="Horizontal">
            <Label Content="Process Name "/>
            <TextBox x:Name="ProcessName" Text="{Binding Path=ModelItem.ProcessName, Mode=TwoWay}"  Width="100" />
            <Label Content=" Window Title "/>
            <TextBox x:Name="WindowTitle" Text="{Binding Path=ModelItem.WindowTitle, Mode=TwoWay}"  Width="200" />
        </StackPanel>
        
    </Grid>
</sap:ActivityDesigner>
